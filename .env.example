# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# JWT Configuration
JWT_SECRET=your-base64-encoded-secret-key-here-must-be-256-bits-minimum
JWT_EXPIRATION=86400000

# Redis Configuration (if not using localhost)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Application Configuration
SHUTTLE_LOCATION_KEY_PREFIX=shuttle:location:

# DTLS Configuration for Raspberry Pi Communication
DTLS_PORT=5684
SHUTTLE_PI_01_PSK=shuttle01SecretKeyChangeInProduction
SHUTTLE_PI_02_PSK=shuttle02SecretKeyChangeInProduction
SHUTTLE_PI_03_PSK=shuttle03SecretKeyChangeInProduction

# Oracle Cloud Configuration (for deployment)
ORACLE_DB_URL=jdbc:oracle:thin:@your-autonomous-db-url
ORACLE_DB_USERNAME=your_oracle_username
ORACLE_DB_PASSWORD=your_oracle_password